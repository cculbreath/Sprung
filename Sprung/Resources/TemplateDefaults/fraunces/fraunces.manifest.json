{
  "sections": {
    "work": {
      "type": "arrayOfObjects",
      "hiddenFields": [
        "description",
        "summary",
        "url",
        "location"
      ]
    },
    "projects": {
      "type": "arrayOfObjects",
      "hiddenFields": [
        "highlights",
        "url",
        "startDate",
        "endDate",
        "keywords",
        "roles",
        "entity",
        "type"
      ]
    },
    "skills": {
      "type": "arrayOfObjects",
      "fields": [
        { "key": "name", "input": "text" },
        { "key": "level", "input": "text" },
        {
          "key": "keywords",
          "input": "chips",
          "allowsManualMutations": true,
          "sourceKey": "skillBank"
        }
      ]
    }
  },
  "custom": {
    "fields": [
      {
        "allowsManualMutations": true,
        "input": "text",
        "key": "jobTitles",
        "repeatable": true
      },
      {
        "input": "textarea",
        "key": "moreInfo"
      }
    ]
  },
  "keys-in-editor": [
    "work",
    "projects",
    "education",
    "skills",
    "custom.jobTitles",
    "custom.moreInfo",
    "styling"
  ],
  "section-visibility": {
    "education": true,
    "projects": true,
    "skills": true,
    "work": true
  },
  "section-visibility-labels": {
    "education": "Education",
    "projects": "Projects",
    "skills": "Skills",
    "work": "Work Experience"
  },
  "sectionOrder": [
    "work",
    "projects",
    "education",
    "skills",
    "custom",
    "styling"
  ],
  "styling": {
    "fontSizes": {
      "boxTitles": "16pt",
      "contact": "10pt",
      "degreeNames": "12pt",
      "employerName": "12pt",
      "moreInfo": "11pt",
      "name": "48pt",
      "projectText": "10.5pt",
      "schools": "10.5pt",
      "sectionTitle": "16pt",
      "skillDescriptions": "10.5pt",
      "skillNames": "11.5pt",
      "summary": "11pt",
      "workDates": "10pt",
      "workHighlights": "10pt",
      "jobTitles": "13pt"
    }
  },
  "transparentKeys": [
    "basics",
    "custom"
  ],
  "editorLabels": {
    "basics": "Contact Info",
    "custom.objective": "Objective",
    "work": "Work Experience",
    "projects": "Projects",
    "education": "Education",
    "skills": "Skills",
    "custom.jobTitles": "Job Titles",
    "custom.moreInfo": "Additional Info"
  },
  "defaultAIFields": [
    "custom.objective",
    "work[].highlights",
    "projects[].description",
    "skills.*.name",
    "skills[].keywords",
    "custom.jobTitles[]"
  ],
  "reviewPhases": {
    "skills": [
      { "phase": 1, "field": "skills.*.name" },
      { "phase": 2, "field": "skills[].keywords" }
    ]
  }
}