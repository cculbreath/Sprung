{
  "sections": {
    "skills": {
      "type": "arrayOfObjects",
      "fields": [
        { "key": "name", "input": "text" },
        { "key": "level", "input": "text" },
        {
          "key": "keywords",
          "input": "chips",
          "allowsManualMutations": true,
          "sourceKey": "skillBank"
        }
      ]
    },
    "education": {
      "default": [
        {
          "custom": {
            "location": "San Luis Obispo, CA"
          }
        }
      ],
      "fields": [
        {
          "input": "text",
          "key": "institution",
          "required": true
        },
        {
          "input": "url",
          "key": "url"
        },
        {
          "input": "text",
          "key": "studyType"
        },
        {
          "input": "text",
          "key": "area"
        },
        {
          "input": "date",
          "key": "startDate"
        },
        {
          "input": "date",
          "key": "endDate"
        },
        {
          "input": "text",
          "key": "score"
        },
        {
          "allowsManualMutations": true,
          "input": "text",
          "key": "courses",
          "repeatable": true
        },
        {
          "children": [
            {
              "input": "text",
              "key": "location",
              "placeholder": "San Luis Obispo, CA"
            }
          ],
          "key": "custom"
        }
      ],
      "titleTemplate": "{{studyType}} in {{area}}",
      "type": "arrayOfObjects"
    }
  },
  "custom": {
    "fields": []
  },
  "keys-in-editor": [
    "basics.label",
    "work",
    "projects",
    "education",
    "skills",
    "styling"
  ],
  "section-visibility": {
    "education": true,
    "projects": true,
    "skills": true,
    "work": true
  },
  "section-visibility-labels": {
    "education": "Education",
    "projects": "Projects",
    "skills": "Skills",
    "work": "Work Experience"
  },
  "sectionOrder": [
    "work",
    "projects",
    "education",
    "skills",
    "custom",
    "styling"
  ],
  "styling": {
    "fontSizes": {
      "date": "10pt",
      "decoratorLine": "10pt",
      "detailHeading": "10pt",
      "detailPlace": "10pt",
      "email": "10pt",
      "mainContent": "10pt",
      "name": "24pt",
      "sectionTitleMain": "12pt",
      "sidebarContent": "10pt",
      "sidebarHeading": "10pt",
      "subtitle": "10pt"
    }
  },
  "transparentKeys": [
    "custom",
    "template"
  ],
  "editorLabels": {
    "basics": "Contact Info",
    "basics.label": "Job Title",
    "custom.objective": "Objective",
    "work": "Work Experience",
    "projects": "Projects",
    "education": "Education",
    "skills": "Skills"
  },
  "defaultAIFields": [
    "custom.objective",
    "work[].highlights",
    "projects[].description",
    "skills.*.name",
    "skills[].keywords",
    "custom.jobTitles"
  ],
  "reviewPhases": {
    "skills": [
      { "phase": 1, "field": "skills.*.name" },
      { "phase": 2, "field": "skills[].keywords" }
    ]
  }
}