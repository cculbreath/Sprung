{
  "sections": {
    "work": {
      "type": "arrayOfObjects",
      "hiddenFields": ["description", "summary", "url"]
    },
    "projects": {
      "type": "arrayOfObjects",
      "hiddenFields": ["url", "startDate", "endDate", "keywords", "roles", "entity", "type"]
    }
  },
  "custom": {
    "fields": [
      {
        "behavior": "sectionLabels",
        "children": [
          {
            "input": "text",
            "key": "work"
          },
          {
            "input": "text",
            "key": "projects"
          },
          {
            "input": "text",
            "key": "education"
          },
          {
            "input": "text",
            "key": "skills"
          }
        ],
        "key": "sectionLabels"
      }
    ]
  },
  "keys-in-editor": [
    "basics.label",
    "work",
    "projects",
    "education",
    "skills",
    "styling"
  ],
  "section-visibility": {
    "education": true,
    "projects": true,
    "skills": true,
    "work": true
  },
  "section-visibility-labels": {
    "education": "Education",
    "projects": "Projects",
    "skills": "Skills",
    "work": "Work Experience"
  },
  "sectionOrder": [
    "work",
    "projects",
    "education",
    "skills",
    "custom",
    "styling"
  ],
  "sections": {
    "education": {
      "default": [
        {
          "custom": {
            "location": "San Luis Obispo, CA"
          }
        }
      ],
      "fields": [
        {
          "input": "text",
          "key": "institution",
          "required": true
        },
        {
          "input": "url",
          "key": "url"
        },
        {
          "input": "text",
          "key": "studyType"
        },
        {
          "input": "text",
          "key": "area"
        },
        {
          "input": "date",
          "key": "startDate"
        },
        {
          "input": "date",
          "key": "endDate"
        },
        {
          "input": "text",
          "key": "score"
        },
        {
          "allowsManualMutations": true,
          "input": "text",
          "key": "courses",
          "repeatable": true
        },
        {
          "children": [
            {
              "input": "text",
              "key": "location",
              "placeholder": "San Luis Obispo, CA"
            }
          ],
          "key": "custom"
        }
      ],
      "titleTemplate": "{{studyType}} in {{area}}",
      "type": "arrayOfObjects"
    }
  },
  "styling": {
    "fontSizes": {
      "date": "10pt",
      "decoratorLine": "10pt",
      "detailHeading": "10pt",
      "detailPlace": "10pt",
      "email": "10pt",
      "mainContent": "10pt",
      "name": "24pt",
      "sectionTitleMain": "12pt",
      "sidebarContent": "10pt",
      "sidebarHeading": "10pt",
      "subtitle": "10pt"
    }
  },
  "transparentKeys": [
    "basics",
    "custom"
  ],
  "editorLabels": {
    "basics": "Contact Info",
    "basics.label": "Job Title",
    "custom.objective": "Objective",
    "work": "Work Experience",
    "projects": "Projects",
    "education": "Education",
    "skills": "Skills"
  },
  "defaultAIFields": [
    "basics.summary",
    "work.*.highlights",
    "projects.*.description",
    "skills.*",
    "skills.*.*",
    "custom.jobTitles.*"
  ],
  "listContainers": [
    "skills.*.keywords",
    "work.*.highlights"
  ],
  "hierarchicalReview": {
    "skills": {
      "enabled": true,
      "phases": ["categoryStructure", "categoryDetails"]
    }
  }
}
