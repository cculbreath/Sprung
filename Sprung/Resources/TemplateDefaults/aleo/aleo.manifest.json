{
  "sections": {
    "work": {
      "type": "arrayOfObjects",
      "hiddenFields": ["description", "summary", "url", "location"]
    },
    "projects": {
      "type": "arrayOfObjects",
      "hiddenFields": ["highlights", "url", "startDate", "endDate", "keywords", "roles", "entity", "type"]
    }
  },
  "custom": {
    "fields": [
      {
        "allowsManualMutations": true,
        "input": "text",
        "key": "jobTitles",
        "repeatable": true
      },
      {
        "input": "textarea",
        "key": "moreInfo"
      },
      {
        "behavior": "sectionLabels",
        "children": [
          {
            "input": "text",
            "key": "work"
          },
          {
            "input": "text",
            "key": "projects"
          },
          {
            "input": "text",
            "key": "education"
          },
          {
            "input": "text",
            "key": "skills"
          }
        ],
        "key": "sectionLabels"
      }
    ]
  },
  "keys-in-editor": [
    "custom.objective",
    "work",
    "projects",
    "education",
    "skills",
    "custom.jobTitles",
    "custom.moreInfo",
    "styling"
  ],
  "section-visibility": {
    "education": true,
    "projects": true,
    "skills": true,
    "work": true
  },
  "section-visibility-labels": {
    "education": "Education",
    "projects": "Projects",
    "skills": "Skills",
    "work": "Work Experience"
  },
  "sectionOrder": [
    "work",
    "projects",
    "education",
    "skills",
    "custom",
    "styling"
  ],
  "styling": {
    "fontSizes": {
      "boxTitles": "16pt",
      "contact": "10pt",
      "degreeNames": "12pt",
      "employerName": "12pt",
      "moreInfo": "11pt",
      "name": "48pt",
      "projectText": "10.5pt",
      "schools": "10.5pt",
      "sectionTitle": "16pt",
      "skillDescriptions": "10.5pt",
      "skillNames": "11.5pt",
      "summary": "11pt",
      "workDates": "10pt",
      "workHighlights": "10pt"
    }
  },
  "transparentKeys": [
    "basics",
    "custom"
  ],
  "editorLabels": {
    "basics": "Contact Info",
    "custom.objective": "Objective",
    "work": "Work Experience",
    "projects": "Projects",
    "education": "Education",
    "skills": "Skills",
    "custom.jobTitles": "Job Titles",
    "custom.moreInfo": "Additional Info"
  },
  "defaultAIFields": [
    "basics.summary",
    "work.*.highlights",
    "projects.*.description",
    "skills.*",
    "skills.*.*",
    "custom.jobTitles.*"
  ],
  "listContainers": [
    "skills.*.keywords",
    "work.*.highlights"
  ],
  "hierarchicalReview": {
    "skills": {
      "enabled": true,
      "phases": ["categoryStructure", "categoryDetails"]
    }
  }
}
